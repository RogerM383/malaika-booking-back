    "paths": {
        "/api/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register new user",
                "description": "Registra un nuevo usuario sin verificar, a la espera de ser verificado por un administrador de Sherwoodmedia.",
                "operationId": "createUser",
                "requestBody": {
                    "description": "Create user",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nif",
                                    "name",
                                    "username",
                                    "email",
                                    "password",
                                    "c_password"
                                ],
                                "properties": {
                                    "nif": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "48965258Z"
                                    },
                                    "name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "Selene"
                                    },
                                    "username": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "selene"
                                    },
                                    "email": {
                                        "type": "email",
                                        "format": "text",
                                        "example": "selene@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "1234567"
                                    },
                                    "c_password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "1234567"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User created successfully"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user with username & pass",
                "description": "Loguea un usuario",
                "operationId": "loginUser",
                "requestBody": {
                    "description": "login user",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "selene@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "1234567"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User loged successfully"
                    }
                }
            }
        },
        "/api/clients": {
            "get": {
                "tags": [
                    "Clients"
                ],
                "summary": "Lista de clientes",
                "description": "Lista los clientes",
                "operationId": "clientList",
                "parameters": [
                    {
                        "name": "client_type",
                        "in": "query",
                        "description": "Id de tipo de cliente",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Nombre del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "surname",
                        "in": "query",
                        "description": "Apellido del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "phone",
                        "in": "query",
                        "description": "Teléfono del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "dni",
                        "in": "query",
                        "description": "DNI del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "passport",
                        "in": "query",
                        "description": "Número de pasaporte del cliente",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Número de elementos por página",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número de página",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Client list retrieves successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/ClientListResource"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Clients"
                ],
                "summary": "Crea un nuevo cliente",
                "description": "Crea un nuevo cliente",
                "operationId": "createClient",
                "requestBody": {
                    "description": "Create user",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "warehouse_id",
                                    "stock"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Selene"
                                    },
                                    "surname": {
                                        "type": "string",
                                        "example": "Selenita"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "6945798"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "selene@gmail.com"
                                    },
                                    "dni": {
                                        "type": "string",
                                        "example": "47854123X"
                                    },
                                    "address": {
                                        "type": "string",
                                        "example": "Caella falsa 123, 08194, Barcelona"
                                    },
                                    "dni_expiration": {
                                        "type": "string",
                                        "example": "2025/12/01"
                                    },
                                    "place_birth": {
                                        "type": "string",
                                        "example": "Barcelona"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Client created successfully"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/api/clients/{id}": {
            "get": {
                "tags": [
                    "Clients"
                ],
                "summary": "Retorna un cliente por ID",
                "description": "Retorna un cliente",
                "operationId": "getClientById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id de cliente",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Client retrived successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/ClientResource"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Clients"
                ],
                "summary": "Actualiza los datos del cliente",
                "description": "Actualiza los datos del cliente",
                "operationId": "updateClient",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id de cliente",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Update client",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "warehouse_id",
                                    "stock"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Selene"
                                    },
                                    "surname": {
                                        "type": "string",
                                        "example": "Selenita"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "6945798"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "selene@gmail.com"
                                    },
                                    "dni": {
                                        "type": "string",
                                        "example": "47854123X"
                                    },
                                    "address": {
                                        "type": "string",
                                        "example": "Caella falsa 123, 08194, Barcelona"
                                    },
                                    "dni_expiration": {
                                        "type": "string",
                                        "example": "2025/12/01"
                                    },
                                    "place_birth": {
                                        "type": "string",
                                        "example": "Barcelona"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Client updated successfully"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
